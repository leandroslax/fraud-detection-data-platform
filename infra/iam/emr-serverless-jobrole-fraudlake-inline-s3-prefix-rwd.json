{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "ListBucketFraudLake",
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket"
            ],
            "Resource": "arn:aws:s3:::fraud-datalake-dev-use1-139961319000"
        },
        {
            "Sid": "ReadWriteDeleteOnPrefixes",
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:DeleteObject",
                "s3:AbortMultipartUpload",
                "s3:ListMultipartUploadParts"
            ],
            "Resource": [
                "arn:aws:s3:::fraud-datalake-dev-use1-139961319000/silver/*",
                "arn:aws:s3:::fraud-datalake-dev-use1-139961319000/gold/*",
                "arn:aws:s3:::fraud-datalake-dev-use1-139961319000/_checkpoints/*"
            ]
        }
    ]
}
